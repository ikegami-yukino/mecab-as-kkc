dictionary-charset = utf-8
cost-factor = 800
bos-feature = BOS/EOS
output-format-type = kkc

; default format
node-format-kkc = %H
unk-format-kkc = %M
eos-format-kkc  = \n

; yet another wakati
node-format-wakachi = %H\s
unk-format-wakachi = %M
eos-format-wakachi = \n

; show also context IDs
node-format-id = %M,%phl,%phr,%H\n
unk-format-id = %M,%phl,%phr,%M,UNK\n
eos-format-id = EOS\n
